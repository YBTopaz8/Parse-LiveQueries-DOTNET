<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <AssemblyName>YBParseLiveQuery</AssemblyName>
    <RootNamespace>YB.Parse.LiveQuery</RootNamespace>
    <!-- NuGet Metadata -->
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>ParseLiveQueryDOTNET</PackageId>
    <Version>1.0.0</Version>
    <Authors>YvanBrunel</Authors>
    <Company>None</Company>
    <Description>A .NET 9.0 library for Parse Live Query. Compatible With .NET MAUI for Windows, Android, MacOS (very very likely iOS too)</Description>
    <PackageTags>Parse, LiveQuery, .NET</PackageTags>
    <RepositoryUrl>https://github.com/YBTopaz8/parse-live-query-dotnet</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Net.WebSockets.Client" Version="4.3.2" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\ParseServer\Parse\Parse.csproj" />
  </ItemGroup>
</Project>
